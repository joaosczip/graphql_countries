{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "DOM.Iterable", "ESNext"],
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "rootDir": "src",
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"]
    },
    "allowJs": true,
    "resolveJsonModule": true
  },
  "include": ["src"],
  "exclude": [""]
}
